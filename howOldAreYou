

import java.util.Scanner;

 class age{
    public static void main(String[] args) {
      Scanner keyboard = new Scanner(System.in);
        System.out.println("How old are you?");
            int userAge = keyboard.nextInt();
        if (userAge<14) {
        System.out.println("You are too young to see a 14A movie without an adult");
        } 
        if (userAge<16) {
        System.out.println("You are too young to get a driver's license");
        } 
        if (userAge<18) {
        System.out.println("You are too young to vote");
        }
        if (userAge<19) {
        System.out.println("You are too young to drink alcohol");
        }
        if (userAge<25) {
        System.out.println("You are too young to rent a car");
        }
        // if you type in a number greater or equal to 25 nothing gets printed because there is no "else" statement.
       
        if (userAge>=65) {
        System.out.println("You are old enough to retire!");
        }
        if (userAge>=14) {
        System.out.println("You are old enough to watch a 14A movie without an adult");}
        if (userAge>=16) {
        System.out.println("You are old enough to get your drivers liscense");    
        }
        if (userAge>=18) {
        System.out.println("You are now old enough to vote");    
        }
        if (userAge>=19) {
        System.out.println("Congradulations you can now drink alchohol");    
        }
        if (userAge>=25) {
        System.out.println("Go out and rent a car, youre old enough now!");
        }
        if (userAge<65) {
        System.out.println("keep working at it. NOT OLD ENOUGH TO RETIRE YET");    
        }
    }
    
}
